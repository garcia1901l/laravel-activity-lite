{
    "name": "garcia1901l/laravel-activity-lite",
    "description": "Lightweight activity logger for Laravel 7-9",
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Frank Garcia",
            "email": "garcia1901l@gmail.com",
            "homepage": "https://github.com/garcia1901l",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/database": "^7.0|^8.0|^9.0",
        "illuminate/events": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0"
    },
    "suggest": {
        "jenssegers/mongodb": "For MongoDB support (Laravel 7.x: 3.7.x, Laravel 8.x: 3.8.x, Laravel 9.x: 3.9.x)",
        "ext-mongodb": "PHP MongoDB extension (^1.10 required)"
    },
    "keywords": [
        "laravel",
        "activity",
        "logger",
        "tracking",
        "mongodb",
        "laravel-7",
        "laravel-8",
        "laravel-9"
    ],
    "config": {
        "platform-check": false,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Garcia1901l\\LaravelActivityLite\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Garcia1901l\\LaravelActivityLite\\ActivityLiteServiceProvider"
            ]
        }
    }
}